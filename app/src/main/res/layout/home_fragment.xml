<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feed.HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/home_TV"
            android:layout_width="408dp"
            android:layout_height="729dp"
            android:text="Hello"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/catagory3_BT"
            android:layout_width="131dp"
            android:layout_height="120dp"
            android:background="@drawable/roundedbutton"
            android:text="Cosmetics"
            android:textSize="12sp"
            android:textStyle="bold"
            app:icon="@drawable/cosmetics"
            app:iconGravity="textTop"
            app:layout_constraintBottom_toBottomOf="@+id/home_TV"
            app:layout_constraintEnd_toStartOf="@+id/catagory4_BT"
            app:layout_constraintHorizontal_bias="0.525"
            app:layout_constraintStart_toStartOf="@+id/home_TV"
            app:layout_constraintTop_toBottomOf="@+id/catagory1_BT"
            app:layout_constraintVertical_bias="0.307" />

        <Button
            android:id="@+id/catagory4_BT"
            android:layout_width="131dp"
            android:layout_height="120dp"
            android:layout_marginEnd="68dp"
            android:background="@drawable/roundedbutton"
            android:text="Hair Styling"
            android:textStyle="bold"
            app:icon="@drawable/salon"
            app:iconGravity="textTop"
            app:layout_constraintBottom_toBottomOf="@+id/home_TV"
            app:layout_constraintEnd_toEndOf="@+id/home_TV"
            app:layout_constraintTop_toBottomOf="@+id/catagory2_BT"
            app:layout_constraintVertical_bias="0.3" />

        <Button
            android:id="@+id/catagory2_BT"
            android:layout_width="131dp"
            android:layout_height="120dp"
            android:layout_marginTop="80dp"
            android:layout_marginEnd="64dp"
            android:background="@drawable/roundedbutton"
            android:text="Leisure"
            android:textStyle="bold"
            app:icon="@drawable/cocktail"
            app:iconGravity="textTop"
            app:layout_constraintEnd_toEndOf="@+id/home_TV"
            app:layout_constraintTop_toBottomOf="@+id/toProflie_BT" />

        <Button
            android:id="@+id/toProflie_BT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Proflie"
            app:layout_constraintBottom_toBottomOf="@+id/home_TV"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.914"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.048" />

        <Button
            android:id="@+id/catagory1_BT"
            android:layout_width="131dp"
            android:layout_height="120dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/roundedbutton"
            android:text="Pedi Meni"
            android:textStyle="bold"
            app:icon="@drawable/nailpolish"
            app:iconGravity="textTop"
            app:layout_constraintEnd_toStartOf="@+id/catagory2_BT"
            app:layout_constraintStart_toStartOf="@+id/home_TV"
            app:layout_constraintTop_toBottomOf="@+id/toProflie_BT" />

        <Button
            android:id="@+id/catagory5_BT"
            android:layout_width="131dp"
            android:layout_height="120dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/roundedbutton"
            android:text="recommend"
            app:icon="@drawable/reward"
            app:iconGravity="textTop"
            app:layout_constraintBottom_toBottomOf="@+id/home_TV"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/home_TV"
            app:layout_constraintTop_toBottomOf="@+id/catagory3_BT"
            app:layout_constraintVertical_bias="0.0" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>